{
  "name": "data-validation-tools",
  "version": "0.0.6",
  "description": "Handy tools",
  "main": "index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "start": "npx ts-node src\\index.ts",
    "test": "jest",
    "build": "tsc && yarn format",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LibrexApp/data-validation-tools.git"
  },
  "author": "andrew pynch, tyler roland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LibrexApp/data-validation-tools/issues"
  },
  "homepage": "https://github.com/LibrexApp/data-validation-tools#readme",
  "dependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-transformer-keys": "^0.4.3",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0"
  }
}
